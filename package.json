{
  "name": "click-outside-element",
  "description": "Trigger event when click outside element or component",
  "version": "0.0.1",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "preview": "vite preview",
    "ts": "tsc",
    "release": "semantic-release",
    "commit": "git-cz",
    "add-and-push": "git add -A && git-cz && git push"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.7",
    "cz-conventional-changelog": "^3.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bas-baskara/click-outside.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "license": "MIT",
  "author": "Baskara Febrianto",
  "publishConfig": {
    "access": "public"
  }
}
